{
  "name": "metasyntactical/google-directions-client",
  "type": "library",
  "description": "Client for Google Directions API to add interpolated points to a route consisting of given coordinates.",
  "keywords": ["Google Maps Directions API", "client", "geography", "routing"],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "David Weichert",
      "email": "info@davidweichert.de"
    }
  ],
  "require": {
    "php": "^7.1",
    "psr/log": "^1.0",
    "beberlei/assert": "^2.9",
    "metasyntactical/http-transport-guzzle": "^1.1",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "codeclimate/php-test-reporter": "^0.4",
    "gamez/psr-testlogger": "^2.0"
  },
  "autoload": {
    "psr-4": { "MetaSyntactical\\GoogleDirections\\": "src/" }
  }
}
